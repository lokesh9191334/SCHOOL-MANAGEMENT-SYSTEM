{
  "version": 2,
  "buildCommand": "cd frontend && npm run build && cd ..",
  "outputDirectory": "frontend/dist",
  "installCommand": "pip install -r requirements.txt",
  "framework": null,
  "functions": {
    "app.py": {
      "runtime": "python3.9",
      "maxDuration": 30
    }
  },
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "/app.py"
    },
    {
      "source": "/auth/(.*)",
      "destination": "/app.py"
    },
    {
      "source": "/static/(.*)",
      "destination": "/app.py"
    },
    {
      "source": "/mobile/(.*)",
      "destination": "/app.py"
    },
    {
      "source": "/ai_features/(.*)",
      "destination": "/app.py"
    },
    {
      "source": "/complaints/(.*)",
      "destination": "/app.py"
    },
    {
      "source": "/class-fees/(.*)",
      "destination": "/app.py"
    },
    {
      "source": "/substitution/(.*)",
      "destination": "/app.py"
    },
    {
      "source": "/timetable/(.*)",
      "destination": "/app.py"
    },
    {
      "source": "/date-sheet/(.*)",
      "destination": "/app.py"
    },
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ]
}